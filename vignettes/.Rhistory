grubbs_test <- function(x, B=2000, min=FALSE, alpha=0.05, ...) { # outliers::grubbs.test(x, type=10)
browser()
stat  <- function(x, index) { abs(x[index]-mean)}
DNAME <- deparse(substitute(x))
index            <- if (min) which.min(x) else which.max(x)
statistic        <- abs(mean(x)-x[index])/sd(x)
names(statistic) <- 'G'
swt <- shapiro.test(x[-index])
if (swt$p.value<alpha) warning("Shapiro wilk test: p.value = ", sprintf("%.6f", swt$p.value))
n <- length(x)
k <- rep(NA, B)
for (i in 1:B) {
x0    <- rnorm(n)
k[i]  <- if (min) min(x0) else max(x0)
}
htest <- list(statistic   = statistic,
p.value     = sum(k>statistic)/B,
method      = 'Grubbs test for one outlier',
alternative = ifelse(min>=max, 'x(1) is an outlier', 'x(n) is an outlier'),
data.name   = DNAME,
statistic0  = k)
class(htest) <- 'htest'
htest
}
beck_test <- function(x, B=2000, min=FALSE, alpha=0.05, ...) { # outliers::grubbs.test(x, type=20)
sqa   <- function(x) { sum((x-mean(x))^2) }
DNAME <- deparse(substitute(x))
args  <- list(...)
n     <- length(x)
z     <- sort(scale(x))
index <- if (min) 1:2 else n:(n-1)
if (is.null(index)) stop ('Either "min" or "max" must be positive')
statistic        <- sqa(z[-index])/sqa(z)
names(statistic) <- 'U'
swt <- shapiro.test(z[-(1:2)])
if (swt$p.value<alpha) warning("Shapiro wilk test: p.value = ", sprintf("%.6f", swt$p.value))
k <- rep(NA, B)
for (i in 1:B) {
z0   <- sort(rnorm(n))
k[i] <- sqa(z0[-index])/sqa(z0)
}
htest <- list(statistic   = statistic,
p.value     = sum(k<statistic)/B,
method      = "Grubbs/Beck test",
alternative = ifelse(minimum, 'x(1) or x(2) is an outlier', 'x(n-1) or x(n) is an outlier'),
data.name   = DNAME,
statistic0  = k)
class(htest) <- 'htest'
htest
}
dhp_test <- function(x, B=2000, alpha=0.05, ...) { # outliers::grubbs.test(x, type=11)
DNAME <- deparse(substitute(x))
args  <- list(...)
statistic        <- diff(range(x))/sd(x)
names(statistic) <- 'G'
swt <- shapiro.test(x[-c(which.min(x), which.max(x))])
if (swt$p.value<alpha) warning("Shapiro wilk test: p.value = ", sprintf("%.6f", swt$p.value))
k <- rep(NA, B)
for (i in 1:B) {
z0   <- rnorm(length(x))
k[i] <- diff(range(z0))/sd(z0)
}
htest <- list(statistic   = statistic,
p.value     = 2*sum(k>statistic)/B,
method      = "David-Hartley-Pearson test",
alternative = 'x(1) or x(n) are outliers',
data.name   = DNAME,
statistic0  = k)
class(htest) <- 'htest'
htest
}
