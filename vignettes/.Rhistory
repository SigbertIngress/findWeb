hist(tst$statistic0, main=fun$name, xlab='statistic (black: simulated, red: sample)',
sub=paste('alternative hypothesis:', tst$alternative), xlim=range(c(tst$statistic0, tst$statistic)))
rug(tst$statistic0)
abline(v=tst$statistic, col="red")
}
}
tst
}
